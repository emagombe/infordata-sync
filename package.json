{
  "name": "infordata-sync",
  "version": "1.0.0",
  "description": "Infordata backup sync",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build": "electron-builder",
    "jsbuild": "webpack --mode=production",
    "dev": "webpack --watch --mode=development"
  },
  "build": {
    "appId": "Infordata backup sync",
    "win": {
      "extraFiles": [
        "config.json"
      ],
      "files": [
        "**/node_modules/**/*",
        "index.js",
        "controller/**/*",
        "service.js",
        "config.js",
        "core.js"
      ]
    },
    "electronDownload": {
      "mirror": "http://127.0.0.1/",
      "cache":"~/.cache/electron/"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emagombe/infordata-sync.git"
  },
  "author": "Infordata Lda",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/emagombe/infordata-sync/issues"
  },
  "homepage": "https://github.com/emagombe/infordata-sync#readme",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/runtime": "^7.8.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "electron": "^7.1.9",
    "electron-builder": "^21.2.0",
    "file-loader": "^4.2.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.0",
    "@material-ui/icons": "^4.5.1",
    "archiver": "^3.1.1",
    "chokidar": "^3.3.1",
    "dateformat": "^3.0.3",
    "ftp": "^0.3.10",
    "mssql": "^6.0.1",
    "node-windows": "^0.1.14",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.2"
  }
}
