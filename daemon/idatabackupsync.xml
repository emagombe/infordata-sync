<service>
	<id>idatabackupsync.exe</id>
	<name>idata Backup Sync</name>
	<description>Syncronize sql Backups with cloud</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<logmode>rotate</logmode>
	<argument>--harmony</argument>
	<argument>--max_old_space_size=4096</argument>
	<argument>C:\Users\emagombe\AppData\Roaming\npm\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>D:\PROJECTS\infordata-sync\service.start.js</argument>
	<argument>--log</argument>
	<argument>idata Backup Sync wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<stoptimeout>30sec</stoptimeout>
	<env name="HOME" value="C:\Users\emagombe"/>
	<env name="TEMP" value="C:\Users\emagombe\temp"/>
	<workingdirectory>D:\PROJECTS\infordata-sync</workingdirectory>
</service>